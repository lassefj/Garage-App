<%- include ('../partials/header') %>
<section class="jumbotron text-center">
    <h1>Create new car</h1>
</section>
<div class="container">
    <form action="/cars/<%= customerId %>" method="POST">
        <div class="form-group">
            <label for="name">Reg.No.</label>
            <input type="text" class="form-control" id="name" name="car[regno]">
        </div>
        <div class="form-group">
            <label for="name">Make</label>
            <input type="text" class="form-control" id="name" name="car[make]">
        </div>
        <div class="form-group">
            <label for="name">Model</label>
            <input type="text" class="form-control" id="name" name="car[model]">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>

<%- include ('../partials/footer') %>